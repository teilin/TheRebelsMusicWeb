{{#if isSecondary}}
    {{#foreach navigation}}
        <li class="list-inline-item"><a href="{{url absolute="true"}}">{{label}}</a></li>
    {{/foreach}}
{{else}}
    {{#foreach navigation}}
        <li><a class="scroll list-inline-item" href="{{url absolute="true"}}">{{label}}</a></li>
    {{/foreach}}
{{/if}}